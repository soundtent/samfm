<div class="entry-page">
    <h1>Entry Page</h1>
    <div class="break"></div>
    <span class="underline"><%= scheduleEntry.location %></span>
    <p><%= scheduleEntry.participants %></p>
    <div class="break"></div>
    <span><%- scheduleEntry.description %></span>
    <div class="break"></div>
    <% if (scheduleEntry.images.length > 0) { %>
        <img class="entry-page__image" src="<%= pathToUrl(scheduleEntry.images[0]) %>">
    <% } else if (scheduleEntry.videos.length > 0) { %>
        <video class="entry-page__video" controls>
            <source src="<%= pathToUrl(scheduleEntry.videos[0]) %>">
        </video> 
    <% } %>
    <div class="break"></div>
    <% scheduleEntry.audios.forEach(audio => { %>
        <iframe style="width: 500px; max-width: 100%; height: 25px;border: none" src="https://max.meetingplaceforsounds.net/hosting/recordingplayer/?playercolor=rgb(46,140,93)&backgroundcolor=white&audiourl=<%= pathToUrl(audio) %>"></iframe> 
        <div class="break"></div>
    <% }) %>
</div>