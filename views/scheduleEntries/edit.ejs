<h1>Edit entry</h1>
<br>
<form id="scheduleEntryForm" action="/schedule-entries/<%= scheduleEntry._id%>?_method=PUT" enctype="multipart/form-data" method="POST">
    <%- include('partials/scheduleEntryForm.ejs') %>
    <input type="submit" value="Update">
</form>
